</main>
<!-- JavaScript to manage active menu item -->
<script>
 document.addEventListener('DOMContentLoaded', function () {
     var currentURL = window.location.pathname; // Get the current path
     var menuItems = document.querySelectorAll('#main-menu .menu-link'); // Select menu links

     menuItems.forEach(function (item) {
         var href = item.getAttribute('href');
         if (currentURL === href) {
             item.parentElement.classList.add('active'); // Add active class to parent (li element)
         } else {
             item.parentElement.classList.remove('active'); // Remove active class from non-matching items
         }
     });
 });
</script>

